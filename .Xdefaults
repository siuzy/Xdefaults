URxvt*title: URxvt
URxvt*name:	URxvt
URxvt*borderLess: false
URxvt*letterSpace: 0
#URxvt*background: #2c2e2f
#URxvt*background: #151515
URxvt*background: #000000
URxvt.foreground: #ffffff
#URxvt*font: -Liberation-Mono-Regular
#URxvt*boldFont: -gohu-gohufont-medium-r-normal--11-80-100-100-c-60-iso8859-1

#URxvt*font: -windows-speedy-medium-r-normal--12-90-96-96-c-70-iso8859-1
#URxvt*boldFont: -windows-speedy-medium-r-normal--12-90-96-96-c-70-iso8859-1


#URxvt*font: -gohu-gohufont-medium-r-normal--11-80-100-100-c-60-iso8859-1
#URxvt*boldFont: -gohu-gohufont-medium-r-normal--11-80-100-100-c-60-iso8859-1

#URxvt*font: -jmk-beep-medium-r-normal--11-100-75-75-c-60-iso8859-1
#URxvt*boldFont: -jmk-beep-medium-r-normal--11-100-75-75-c-60-iso8859-1
#URxvt*italicFont: -jmk-beep-medium-r-normal--11-100-75-75-c-60-iso8859-1
#URxvt*boldItalicFont: -jmk-beep-medium-r-normal--11-100-75-75-c-60-iso8859-1

URxvt.perl-ext-common: matcher,selection-to-clipboard
URxvt.url-launcher: firefox
URxvt.matcher.button: 1

URxvt*saveLines: 10000
URxvt*scrollBar: false

! do not scroll with output
URxvt*scrollTtyOutput: false

! scroll in relation to buffer (with mouse scroll or Shift+Page Up)
URxvt*scrollWithBuffer: true

! scroll back to the bottom on keypress
URxvt*scrollTtyKeypress: true

URxvt.secondaryScreen: 1
URxvt.secondaryScroll: 0

URxvt.keysym.Home: \033[1~
URxvt.keysym.End: \033[4~
URxvt.keysym.KP_Home: \033[1~
URxvt.keysym.KP_End:  \033[4~

URxvt.internalBorder: 10
URxvt*.cutchars: "()*,;<>[]{}|`\"'#â”‚"'`"

